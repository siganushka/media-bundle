{
    "name": "siganushka/media-bundle",
    "description": "Media Bundle.",
    "keywords": ["media-bundle"],
    "homepage": "https://github.com/siganushka/media-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "siganushka",
            "email": "siganushka@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "doctrine/doctrine-bundle": "^2.7",
        "knplabs/knp-paginator-bundle": "^5.9",
        "siganushka/doctrine-contracts": "dev-main",
        "siganushka/generic-bundle": "dev-main",
        "siganushka/registry-contracts": "dev-main",
        "symfony/form": "^5.4|^6.0",
        "symfony/framework-bundle": "^5.4|^6.0",
        "symfony/polyfill-php80": "^1.28",
        "symfony/serializer": "^5.4|^6.0",
        "symfony/validator": "^5.4|^6.0"
    },
    "require-dev": {
        "aliyuncs/oss-sdk-php": "^2.6",
        "symfony/phpunit-bridge": "^6.0",
        "vimeo/psalm": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Siganushka\\MediaBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Siganushka\\MediaBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
